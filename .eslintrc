{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      1,
      "always",
      {
        "omitLastInOneLineBlock": true
      }
    ],
    "no-undef": [
      0
    ],
    "no-unused-vars": [
      0
    ],
    "react/react-in-jsx-scope": 2,
    "react/jsx-no-undef": [
      0
    ],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/display-name": 0
  },
  "plugins": [
    "react"
  ]
}
